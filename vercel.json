{
    "version": 2,
    "builds": [
        {
            "src": "api.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9",
                "buildCommand": "pip install --upgrade pip setuptools wheel && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api.py"
        }
    ]
} 